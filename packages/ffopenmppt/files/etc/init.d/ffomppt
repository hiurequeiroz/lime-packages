#!/bin/sh /etc/rc.common

START=95
STOP=95



boot() {

pid=$!

/usr/bin/collect-data.sh & 

pid=$!

echo $pid > /tmp/collect-data-pid

/usr/bin/powersave.sh &

pid=$!

echo $pid > /tmp/powersave-pid

}

stop() {

kill `cat /tmp/collect-data-pid`
kill `cat /tmp/powersave-pid`
rm /tmp/collect-data-pid
}
